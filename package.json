{
  "name": "generator-nodejs-microservice-starter",
  "version": "1.2.0",
  "description": "Generarate a NodeJS RESTful API Microservice Starter",
  "homepage": "https://github.com/Abdizriel/nodejs-microservice-starter",
  "author": {
    "name": "Marcin Mrotek",
    "email": "kontakt@marcinmrotek.pl",
    "url": "https://github.com/Abdizriel"
  },
  "main": "app/index.js",
  "keywords": [
    "generator",
    "nodejs",
    "microservice",
    "mongodb",
    "express",
    "yeoman-generator"
  ],
  "dependencies": {
    "yeoman-generator": "^0.23.0",
    "chalk": "^1.0.0",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.3.13",
    "yeoman-test": "^1.0.0",
    "yeoman-assert": "^2.0.0",
    "eslint": "^2.1.0",
    "eslint-config-xo-space": "^0.13.0",
    "chai": "^3.3.0",
    "coveralls": "^2.11.8",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "*",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Abdizriel/generator-nodejs-microservice-starter"
  },
  "bugs": {
    "url": "https://github.com/Abdizriel/generator-nodejs-microservice-starter/issues"
  },
  "scripts": {
    "prepublish": "npm t",
    "test": "mocha test/ --recursive -R dot --check-leaks --require babel-core/register",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/.bin/_mocha test/  --report lcovonly -- --recursive -R min --check-leaks --require babel-core/register && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  }
